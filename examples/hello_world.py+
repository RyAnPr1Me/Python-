# Hello World in Python++
# This file demonstrates the .py+ extension

def greet(name):
    return f"Hello, {name}! Welcome to Python++"

if __name__ == "__main__":
    print(greet("World"))
    print("Python++ is Python with AOT compilation!")
    
    # Show some basic operations
    numbers = [1, 2, 3, 4, 5]
    squared = [n**2 for n in numbers]
    print(f"Squares: {squared}")
    
    # Sum calculation
    total = sum(numbers)
    print(f"Sum of {numbers} = {total}")
